!!! 5
%html
  %head
    %title= t(:'site.logo_text')
    = stylesheet_link_tag    "application"
    = javascript_include_tag "application"
    = csrf_meta_tags

  %body{:style => "padding-top: 60px"}
    .navbar.navbar-fixed-top
      .navbar-inner
        .container
          = link_to t(:'site.logo_text'), root_path, :class => 'brand'
          .nav-collapse
            %ul.nav
              = render :partial => '/shared/admin_top_nav'




    .container
      .content
        = yield

